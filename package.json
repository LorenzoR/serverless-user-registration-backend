{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "offline": "serverless offline --httpPort 3005 --stage local",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "buildtest": "tsc --noEmit",
    "dynamodb": "sls dynamodb start --migrate --stage local",
    "jest": "NODE_ENV=test ./node_modules/.bin/jest --ci --verbose --coverage --forceExit --detectOpenHandles",
    "test": "sls dynamodb start --migrate --stage local & sleep 10 && npm run jest && kill -9 `lsof -t -i:8000`"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.697.0",
    "bcrypt": "^5.0.0",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/aws-lambda": "^8.10.56",
    "@types/jest": "^25.2.3",
    "@types/node": "^10.17.26",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-module-resolver": "^0.16.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.0.1",
    "serverless-create-global-dynamodb-table": "^2.1.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-offline": "^6.4.0",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.9.5",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
